<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>

<GridPane 
	alignment="CENTER" 
	hgap="10.0" 
	vgap="10.0"
    xmlns:fx="http://javafx.com/fxml"
    fx:controller="test.fxml.addressbook.FXMLTableViewController">
    <padding>
        <Insets 
        	bottom="10.0" 
        	left="10.0" 
        	right="10.0" 
        	top="10.0" />
    </padding> 
    <Label 
    	fx:id="label"
    	style="-fx-font: NORMAL 20 Tahoma;" 
    	text="Address Book"                
        GridPane.columnIndex="0" 
        GridPane.rowIndex="0">
    </Label>
    <TableView 
    	fx:id="tableView" 
    	GridPane.columnIndex="0" 
        GridPane.rowIndex="1" />
</GridPane>

